{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": 1,
      "no-console": 0,
      "no-plusplus": 0,
      "no-debugger": 1,
      "no-param-reassign": 0,
      "no-constant-condition": 0
    }
}